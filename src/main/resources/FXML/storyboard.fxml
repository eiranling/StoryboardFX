<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="464.0" prefWidth="224.0" styleClass="storyboard" stylesheets="@../css/storyboard.css, @../css/generic.css" type="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox layoutY="5.0" prefHeight="25.0" prefWidth="224.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="storyboardTitle" text="Untitled">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" top="2.0" />
               </HBox.margin>
            </Label>
            <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="90.0" spacing="4.0" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets right="3.0" />
               </padding>
               <children>
                  <Button mnemonicParsing="false" onAction="#addStory" text="+" styleClass="storyboard-button">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
      <VBox fx:id="storyContainer" layoutY="30.0" prefHeight="434.0" prefWidth="224.0" styleClass="container" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0" />
   </children>
</fx:root>
